	body
		if user
			div.header
				div
					button#trigger(src='/img/hamb.png' alt='Menu Icon')
			div.headerDropDown#hdd
				ul.headerUl
					li.headerLi
						form(action='/')
							button.hov Home
					li.headerLi  
						form(action='/db/storeview')
							button.hov View Drystore
					li.headerLi
						form(action='/db/add')
							button.hov Add Item
					li.headerLi
						form(action='/insights/console')
							button.hov Insights
					li.headerLi
						form(action='/users/confirmlogout')
							button.hov Log Out

		if !user
			div.header
				div
					button#trigger(src='/img/hamb.png' alt='Menu Icon')
			div.headerDropDown#hdd
				ul.headerUl.shorter
					li.headerLi
						form(action='/')
							button.hov Home
					li.headerLi
						form(action='/users/login')
							button.hov Login
		div.headBreaker
		input#data(type='hidden', value=items)
		h1.heading.bigHead Insights
